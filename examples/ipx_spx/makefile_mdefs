HERE: src/ex1.mmd.def
$(DISC_PATH)/ex1.mmd: shared_mmd_layout.s.o ex1.c.o ex1_res.s.o 
	$(call link_module, $^, $@, $(CFG_PATH)/module_mmd.ld, ipx.mmd)


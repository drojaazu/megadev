	$(call link_module, $^, $@, $(CFG_PATH)/module_mmd.ld, ipx.mmd)

HERE: src/ex3.mmd.def
$(DISC_PATH)/ex3.mmd: shared_mmd_layout.s.o ex3.c.o ex3_res.s.o 
	$(call link_module, $^, $@, $(CFG_PATH)/module_mmd.ld, ipx.mmd)

HERE: src/ipx.mmd.def
$(DISC_PATH)/ipx.mmd: ipx_layout.s.o ipx_init.s.o ipx.c.o 
	$(call link_module, $^, $@, $(CFG_PATH)/module_mmd.ld, ip.bin)

HERE: src/spx.smd.def
